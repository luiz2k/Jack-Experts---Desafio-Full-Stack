{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "engines": {
    "node": "20.14.0"
  },
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "start": "node dist/server.js",
    "dev": "tsx --watch src/server.ts",
    "test": "vitest",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/database/data-source ./src/database/migrations/init",
    "migration:run": "npm run typeorm migration:run -- -d ./src/database/data-source",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/database/data-source"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@types/swagger-ui-express": "^4.1.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.18.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
